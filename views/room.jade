extends layout

block content
  h1= title
    form(method='POST')
      input(type='input', name='message')
  #msgContainer
    ul
      each message in body
        li= message
  h1 Rooms
    form(method='POST')
      input(type='input', name='room')
  #rmContainer
    ul
      each room in rooms
        li
          a(href='/' + room)=room

